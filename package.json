  "lint-staged": {
    "**/*.js": [
      "yarn prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "yarn eslint --ext ts,tsx --fix"
    ],
    "**/*.{css,scss}": [
      "yarn prettier --write"
    ]
  },
