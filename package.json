  "license": "MIT",
  "scripts": {
    "start": "npm run build-dll-dev && npm run start-lite",
    "start-lite": "webpack-dev-server --progress --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production npm run build-dll && cross-env NODE_ENV=production webpack --config=webpack.config.prod.js",
    "build-lite": "cross-env NODE_ENV=production webpack --config=webpack.config.prod.js",
    "build-dev": "npm run build-dll-dev && npm run build-dev-lite",
    "build-dev-lite": "webpack --config=webpack.config.dev.js",
    "build-stats": "cross-env NODE_ENV=production webpack --config=webpack.config.prod.js --profile --json > stats.json",
    "build-dll": "webpack --config webpack.config.dll.js -p --hide-modules --mode=production",
    "build-dll-dev": "webpack --config webpack.config.dll.js --hide-modules --mode=development",
    "format:check": "yarn prettier --check \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "format:fix": "yarn prettier --write \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "lint:check": "yarn eslint --ext ts,tsx src",
    "lint:fix": "yarn eslint --fix --ext ts,tsx src",
    "test": "yarn format:check && yarn lint:check && jest --verbose --runInBand && npm run build",
    "ci:conf": "mkdirp build/scripts/configs/ && cp src/configs/test-config.json build/scripts/configs/config.json",
    "ci:start": "yarn build && yarn ci:conf && yarn serve build -l 8001",
    "ci:run": "cypress run",
    "ci:test": "start-server-and-test ci:start http://localhost:8001/ ci:run"
  },
