    "webpack-merge": "^5.8.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.js": [
      "yarn prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "yarn eslint --ext ts,tsx --fix"
    ],
    "**/*.{css,scss}": [
      "yarn prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "suiteNameTemplate": "{filepath}"
  },
  "knip": {
    "cypress": {
      "config": "cypress.config.ts",
      "entry": [
        "cypress/e2e/**/*.ts",
        "cypress/support/e2e.ts",
        "cypress/support/commands.ts"
      ]
    },
    "eslint": {
      "entry": [
        ".eslintrc.js",
        "cypress/.eslintrc.js"
      ]
    },
    "webpack": {
      "config": [
        "webpack.config.*.js"
      ]
    },
    "ignore": [
      "extractLiteralsFromFiles.js",
      "src/SettingsDescription.ts"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "ignoreFiles": [
      "./src/metronic/**/*.scss"
    ],
    "rules": {
      "no-descending-specificity": null,
      "scss/load-no-partial-leading-underscore": null
    }
  },
  "jscpd": {
    "ignore": [
      "src/form/BoxNumberField.scss",
      "src/metronic/sass/core/components/buttons/_theme.scss",
      "src/metronic/sass/core/base/functions/_get.scss"
    ]
  }
}
